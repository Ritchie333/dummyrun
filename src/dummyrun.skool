; Clear the inventory display
;
; Used by the routine at #R$A242.
c$A220 EXX           ; Exchange main registers.
 $A221 LD IX,$F332   ; Point #REGix at the screen offset table.
 $A225 LD C,$28      ; #N$28 lines to clear.
*$A227 LD L,(IX+$00) ; { Put the screen address in #REGhl.
 $A22A LD H,(IX+$01) ; }
 $A22D LD D,$00      ; { Add #N$10 to point to the centre of the row.
 $A22F LD E,$10      ; 
 $A231 ADD HL,DE     ; }
 $A232 LD B,$10      ; #N$10 columns to clear.
*$A234 LD (HL),$00   ; Clear the byte.
 $A236 INC HL        ; Move to the next column.
 $A237 DJNZ $A234    ; Loop until all columns clear.
 $A239 INC IX        ; { Move to the next position in the table.
 $A23B INC IX        ; }
 $A23D DEC C         ; Decrement the row count.
 $A23E JR NZ,$A227   ; Loop while there are more rows to clear.
 $A240 EXX           ; { Exchange main registers and return.
 $A241 RET           ; }

